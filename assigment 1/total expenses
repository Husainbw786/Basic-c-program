#include <stdio.h>
int main(){
int quantity, discount=0;
float rate,expense;
printf("enter the quantity\n");
scanf("%d",&quantity);
printf("enter rate \n");
scanf("%f",&rate);

if (quantity>=1000)
{
     
     expense = (quantity*rate);
     expense = (quantity*rate) - ((expense/100)*10);
}
else
expense = (quantity*rate);
printf("total expense is = %f\n",expense);
