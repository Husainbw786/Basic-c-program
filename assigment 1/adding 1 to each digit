#include <stdio.h>
int main(){
    int first_d,second_d,third_d,fourth_d,fifth_d,input,result;
    printf("-----enter a five digit number------\n");
    scanf("%d",&input);
    
    first_d   = input/10000;
    second_d  = (input/1000)%10;
    third_d   = (input%1000)/100;
    fourth_d  = (input%100)/10;
    fifth_d   = (input%10);
    
    result = (first_d + 1)*10000 + (second_d + 1)*1000 +(third_d + 1)*100 + (fourth_d + 1)*10 + (fifth_d + 1);
    printf("the obatined value is = %d",result);
    
}
