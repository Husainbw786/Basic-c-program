
#include <stdio.h>
int main(){
    int num1,num2;
    printf("-----swapping using xor\n");
    printf("enter the first number\n");
    scanf("%d",&num1);
    printf("enter the second number\n");
    scanf("%d",&num2);
     
    num1 = num1^num2;
    num2 = num2^num1;
    num1 = num1^num2;
    printf("num1 after swapping %d\n",num1);
    printf("num2 after swapping %d\n",num2);
    
    
}
