#include <stdio.h>
int main()
{
    int rows,coloumn;
    printf("enter the number of rows and coloumn\n");
    scanf("%d%d",&rows,&coloumn);
    printf("\n");
      
    if(rows > 0 && coloumn > 0)
    {
     for(int i=1;i <= rows;i++)
      {
        for(int j=1;j <= coloumn;j++)
         {
           if(i==1 || j==1 || i == rows || j == coloumn)
               printf("* ");
           else 
               printf("  ");
                                     }
               printf("\n");
            
                                            }
                                                  }
    
}
