#include <stdio.h>
int main ()
{
  int num,sum,firstdigit,lastdigit;
  
  printf("enter any four digit number to find sum of first and last digit\n");
  scanf("%d",&num);
   lastdigit = num % 10;
   
   firstdigit = num / 1000;
   
   sum = firstdigit + lastdigit;
   
   printf("sum of first and last digit =%d",sum);
}
   
   
   
