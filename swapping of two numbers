
#include <stdio.h>
int main(){
    int a,b,c;
    printf("swaping of two numbers\n");
    printf("enter first number\n");
    scanf("%d",&a);
    printf("enter second number\n");
    scanf("%d",&b);
    
    a = b;
    b = c;
    c = a;
    printf("after swaping first number = %d",a);
    printf("after swaping second number = %d",c);
    

}

2nd methord
#include <stdio.h>

int main() {
    int a = 10,b = 20;

    printf("----before swapping a =%d b =%d\n",a,b);
    
    a = a+b;
    b = a-b;
    a = a-b;
    printf("After swap a=%d b=%d\n",a,b);
    
    return 0;
}

3rd methord
#include <stdio.h>

int main() {
    int a = 10,b = 20;

    printf("----before swapping a =%d b =%d\n",a,b);
    
    a = a*b;
    b = a/b;
    a = a/b;
    printf("After swap a=%d b=%d\n",a,b);
    
    return 0;
}
