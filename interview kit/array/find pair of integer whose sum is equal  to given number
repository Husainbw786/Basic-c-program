
#include <iostream>

using namespace std;

int main()
{
    int i,j,n;
    int sum;
    cin >> n;
    cin >> sum;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin >> a[i];
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]+a[j] == sum)
            {
                cout <<" \n " << a[i] << " " <<  a[j] << "\n";
            }
        }
    }
    
    return 0;
}
