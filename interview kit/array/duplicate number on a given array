how to find the duplicate number on a given integer  array.
********************ANS*************************************

int main()
{
    int i,j,n;
    cin >> n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin >> a[i];
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i] == a[j])
            {
                cout << "duplicate number is" <<" \n " << a[i] << "\n";
            }
        }
    }
    


    

    return 0;
}
