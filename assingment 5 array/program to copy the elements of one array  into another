program to copy the elements of one array into another array








#include <stdio.h>

int main() {
   int size,i,arr1[10],arr2[10];
   printf("program  to copy the elements of one array into another array\n");
   printf("enter the size of an array\n");
   scanf("%d",&size);
   printf(" enter the number of elements in array\n");
     for(i=0; i<size; i++)
        {
          
            scanf("%d",&arr1[i]);
        }
        
        
    for (i=0; i< size; i++)
   {  arr2[i] = arr1[i];
     printf(" %d %d ", arr1[i],arr2[i]);
   }
    return 0;
}
